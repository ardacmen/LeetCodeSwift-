class Solution {
    func twoSum(_ nums: [Int], _ target: Int) -> [Int] {
        
      
        
        for i in 0..<nums.count-1{
            for j in i..<nums.count{
                if j == 0 {
                    continue
                }else{
                     if (nums[i] + nums[j] == target)
                        {
                         return [i,j]   
                        }
                }
              
            }
        }
      return [1,1]

    }
}
